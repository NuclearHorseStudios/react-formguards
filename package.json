{
  "name": "react-formguards",
  "version": "1.1.1",
  "description": "Simple client side form validation",
  "author": "Michael Lasky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaellasky/react-formguards.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "typings",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --transformIgnorePatterns '/node_modules/' --env=jsdom --coverage",
    "test:watch": "react-scripts test --transformIgnorePatterns '/node_modules/' --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "@types/react": "^16.8.22"
  },
  "devDependencies": {
    "@babel/runtime": "^7.8.4",
    "@svgr/rollup": "^2.4.1",
    "@testing-library/react": "^8.0.9",
    "@testing-library/user-event": "^4.2.4",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.6.1",
    "cross-env": "^5.2.1",
    "eslint": "^6.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^1.4.0"
  },
  "files": [
    "dist",
    "typings"
  ],
  "bugs": {
    "url": "https://github.com/michaellasky/react-formguards/issues"
  },
  "homepage": "https://github.com/michaellasky/react-formguards#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "react",
    "declarative",
    "client",
    "side",
    "form",
    "validation"
  ],
  "dependencies": {
    "deepmerge": "^4.2.2"
  }
}
